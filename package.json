{
  "name": "medlink",
  "version": "0.0.1",
  "description": "MedLink - Medical Management System",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"pnpm -F web dev\" \"pnpm -F backend start:dev\"",
    "start": "concurrently \"pnpm -F web preview\" \"pnpm -F backend start:prod\"",
    "build": "pnpm -r build",
    "build:backend": "pnpm -F backend build",
    "build:web": "pnpm -F web build",
    "lint": "pnpm -r lint",
    "test": "pnpm -F backend test",
    "test:e2e": "pnpm -F backend test:e2e",
    "install:all": "pnpm install -r",
    "ci": "pnpm install -r --frozen-lockfile",
    "clean": "pnpm run clean:modules && pnpm run clean:build",
    "clean:modules": "rm -rf node_modules backend/node_modules web/node_modules pnpm-lock.yaml",
    "clean:build": "rm -rf backend/dist web/dist",
    "reinstall": "pnpm run clean:modules && pnpm install -r",
    "docker:up": "docker-compose up -d",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "docker:clean": "docker-compose down -v"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "packageManager": "pnpm@10.14.0+sha512.ad27a79641b49c3e481a16a805baa71817a04bbe06a38d17e60e2eaee83f6a146c6a688125f5792e48dd5ba30e7da52a5cda4c3992b9ccf333f9ce223af84748"
}
